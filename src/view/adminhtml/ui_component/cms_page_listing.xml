<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="export">
                <settings>
                    <confirm>
                        <message translate="true">Are you sure you want to export selected items?</message>
                        <title translate="true">Export to data patches</title>
                    </confirm>
                    <url path="dataPatchCreator/massExport/CmsPage"/>
                    <type>export</type>
                    <label translate="true">Export To DataPatches</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
